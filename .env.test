# Test Environment Configuration
# This file is used by Playwright tests

POSTGRES_DATABASE=test_db
POSTGRES_HOST=localhost
POSTGRES_PASSWORD=test
POSTGRES_PRISMA_URL=sqlite::memory:
# Test database URL - using in-memory SQLite for isolation
POSTGRES_URL=sqlite::memory:
POSTGRES_URL_NON_POOLING=sqlite::memory:
POSTGRES_URL_NO_SSL=sqlite::memory:
POSTGRES_USER=test

# Test auth secret
AUTH_SECRET=test-auth-secret-for-e2e-testing-only

ANTHROPIC_API_KEY=
COHERE_API_KEY=
GOOGLE_GENERATIVE_AI_API_KEY=
GROQ_API_KEY=
# Disable external services for testing
# Leave these empty to disable AI providers in test mode
OPENAI_API_KEY=
XAI_API_KEY=

# Test blob storage
BLOB_READ_WRITE_TOKEN=test-blob-token

# Test Redis URL (will use memory cache fallback)
REDIS_URL=redis://localhost:6379

# Disable analytics for tests
LANGCHAIN_API_KEY=
NEXT_PUBLIC_POSTHOG_HOST=
NEXT_PUBLIC_POSTHOG_KEY=

# Test environment
NODE_ENV=test
PLAYWRIGHT=true

# Disable telemetry
NEXT_TELEMETRY_DISABLED=1

# Use test port
PORT=3001
