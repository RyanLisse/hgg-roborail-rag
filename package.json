{
  "name": "hgg-roborail-assistant",
  "version": "3.0.23",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:railway": "pnpm db:migrate && next build",
    "start": "next start",
    "lint": "next lint && biome lint",
    "lint:fix": "next lint --fix && biome lint --write --unsafe",
    "format": "biome format --write",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "npx tsx lib/db/migrate.ts",
    "db:migrate:test": "npx tsx lib/db/migrate-test.ts",
    "db:studio": "drizzle-kit studio",
    "db:studio:test": "drizzle-kit studio --config=drizzle.config.test.ts",
    "db:push": "drizzle-kit push",
    "db:push:test": "drizzle-kit push --config=drizzle.config.test.ts",
    "db:pull": "drizzle-kit pull",
    "db:pull:test": "drizzle-kit pull --config=drizzle.config.test.ts",
    "db:check": "drizzle-kit check",
    "db:check:test": "drizzle-kit check --config=drizzle.config.test.ts",
    "db:up": "drizzle-kit up",
    "test": "export PLAYWRIGHT=True && pnpm run kill:dev && pnpm exec playwright test",
    "test:stagehand": "export PLAYWRIGHT=True && pnpm run kill:dev && pnpm exec playwright test --project=stagehand",
    "test:traditional": "export PLAYWRIGHT=True && pnpm run kill:dev && pnpm exec playwright test --project=e2e",
    "test:quick": "export PLAYWRIGHT=True && pnpm run kill:dev && pnpm exec playwright test --workers=1 --timeout=15000",
    "kill:dev": "lsof -ti:3000,3001 | xargs kill -9 2>/dev/null || echo 'No dev servers running'",
    "test:e2e": "export PLAYWRIGHT=True && pnpm exec playwright test",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:vectorstore": "vitest lib/vectorstore/__tests__",
    "test:agents": "vitest lib/agents/__tests__",
    "test:comprehensive": "./scripts/run-comprehensive-tests.sh",
    "test:vectorstores": "tsx tests/scripts/test-vector-stores.js",
    "vectorstore:test": "node tests/scripts/test-vector-stores.js",
    "vectorstore:summary": "node tests/scripts/vector-store-summary.js",
    "vectorstore:openai": "node tests/scripts/simple-openai-test.js",
    "vectorstore:files": "node tests/scripts/get-vectorstore-files.js",
    "vectorstore:search": "node tests/scripts/test-vector-search.js",
    "test:roborail": "node scripts/test-roborail-responses.js",
    "test:responses": "node tests/scripts/test-openai-responses.js",
    "test:relevance": "node scripts/test-relevance-scoring.js",
    "test:db": "node tests/database-validation.js",
    "test:db:setup": "pnpm run db:migrate:test && pnpm run test:db",
    "db:health": "node scripts/db-health-monitor.js check",
    "db:monitor": "node scripts/db-health-monitor.js monitor"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/cohere": "^1.2.10",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/react": "^1.2.12",
    "@ai-sdk/xai": "^1.2.18",
    "@codemirror/lang-javascript": "^6.2.4",
